<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/more"
    tools:context=".ui.more.More">


    <com.google.android.material.button.MaterialButton
        android:id="@+id/BMIbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@android:color/holo_orange_dark"
        android:layout_centerHorizontal="true"
        android:layout_margin="60dp"
        android:text="Lean Body Mass Calculator"
        />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/WILKSbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BMIbutton"
        android:backgroundTint="@android:color/holo_orange_dark"
        android:layout_centerHorizontal="true"
        android:layout_margin="60dp"
        android:text="Wilks Calculator"
        />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/KGbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/WILKSbutton"
        android:backgroundTint="@android:color/holo_orange_dark"
        android:layout_centerHorizontal="true"
        android:layout_margin="60dp"
        android:text="KG/LBS Conversion"
        />


</RelativeLayout>